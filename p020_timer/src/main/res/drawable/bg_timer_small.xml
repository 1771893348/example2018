<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/x1" android:color="@color/timer_bg_small" />
            <solid android:color="@color/timer_bg_small" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/x1" android:color="@color/my_sure" />
            <solid android:color="@color/my_sure" />
            <corners android:radius="5dp" />
        </shape>
    </item>
</selector>