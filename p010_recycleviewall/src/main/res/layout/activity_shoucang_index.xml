<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E9E9E9"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <FrameLayout
            android:id="@+id/sc_page_0_item_0"
            android:layout_width="@dimen/x400"
            android:layout_height="@dimen/y616"
            android:layout_marginLeft="@dimen/x32"
            android:layout_marginTop="@dimen/x32">

        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/y616"
            android:layout_marginLeft="@dimen/x32"
            android:layout_marginRight="@dimen/x32"
            android:layout_marginTop="@dimen/x32"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/x16"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/sc_page_0_item_1"
                    android:layout_width="@dimen/x376"
                    android:layout_height="@dimen/y292"
                    android:layout_marginRight="@dimen/x16">

                </FrameLayout>

                <FrameLayout
                    android:id="@+id/sc_page_0_item_2"
                    android:layout_width="@dimen/x376"
                    android:layout_height="@dimen/y292"
                    android:layout_marginLeft="@dimen/x16">

                </FrameLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x16"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/sc_page_0_item_3"
                    android:layout_width="@dimen/x376"
                    android:layout_height="@dimen/y292"
                    android:layout_marginRight="@dimen/x16">

                </FrameLayout>

                <FrameLayout
                    android:id="@+id/sc_page_0_item_4"
                    android:layout_width="@dimen/x376"
                    android:layout_height="@dimen/y292"
                    android:layout_marginLeft="@dimen/x16">

                </FrameLayout>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

    <!--footer-->
    <LinearLayout
        android:id="@+id/bottom"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y136"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="@dimen/x26"
        android:layout_marginRight="@dimen/x26"
        android:orientation="horizontal">

        <include
            layout="@layout/new_bottom_back_include2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/footer_bgs"
            android:gravity="center_vertical"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/x10"
                android:layout_marginLeft="@dimen/x50">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="我的收藏"
                    android:textColor="@color/white"
                    android:textSize="@dimen/x30"
                    android:textStyle="bold" />

            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>

</RelativeLayout>